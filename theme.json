{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "820px",
            "wideSize": "1100px"
        },
        "color": {
            "palette": [
                {
                    "slug": "primary",
                    "color": "#63acb7",
                    "name": "Primary"
                },
                {
                    "slug": "secondary",
                    "color": "#323232",
                    "name": "Secondary"
                },
                {
                    "slug": "dark",
                    "color": "#191919",
                    "name": "Dark"
                },
                {
                    "slug": "light",
                    "color": "#F2F2F2",
                    "name": "Light"
                },
                {
                    "slug": "white",
                    "color": "#ffffff",
                    "name": "White"
                },
                {
                    "slug": "gray",
                    "color": "#7f7f7f",
                    "name": "Gray"
                },
                {
                    "slug": "accent",
                    "color": "#c53929",
                    "name": "Accent"
                }
            ],
            "gradients": [],
            "duotone": []
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "\"Lato\", \"游ゴシック体\", \"Yu Gothic\", \"YuGothic\", \"ヒラギノ角ゴシック Pro\", \"Hiragino Kaku Gothic Pro\", \"メイリオ\", \"Meiryo\", \"Osaka\", \"ＭＳ Ｐゴシック\", \"MS PGothic\", sans-serif",
                    "slug": "body",
                    "name": "Body"
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "1.2rem",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "1.4rem",
                    "name": "Normal"
                },
                {
                    "slug": "medium",
                    "size": "1.6rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "2rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "2.8rem",
                    "name": "Extra Large"
                }
            ]
        },
        "spacing": {
            "padding": true,
            "margin": true,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ]
        },
        "border": {
            "radius": true,
            "color": true,
            "style": true,
            "width": true
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--light)",
            "text": "var(--wp--preset--color--dark)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--body)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontWeight": "500",
            "lineHeight": "1.75"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--secondary)"
                    }
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "900"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "700"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "700"
                }
            }
        },
        "blocks": {
            "core/button": {
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--white)"
                },
                "border": {
                    "radius": "5px"
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ],
    "customTemplates": [],
    "patterns": []
}